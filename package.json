{
    "name": "atma-loader-postmask",
    "version": "1.0.12",
    "description": "Mask optimizers for `atma-io`, `IncludeJS` and `atma-server`.",
    "author": "Alexander Kit (alex.kit@atmajs.com)",
    "main": "index.js",
    "license": "MIT",
    "scripts": {
        "build": "app-bundler",
        "server": "atma server"
    },
    "dependencies": {
        "atma-io-middleware-base": "^1.0.11",
        "maskjs": ">=0.54.33",
        "postmask": "^1.1.5"
    },
    "devDependencies": {
        "app-bundler": "0.0.35",
        "atma": "latest",
        "atma-loader-ts": "^1.0.0"
    },
    "app-bundler": {
        "file": "src/index.ts",
        "outputMain": "index.js",
        "package": {
            "module": "commonjs",
            "commonjs": {
                "output": "simplified"
            }
        },
        "defaultExtensions": {
            "js": "ts"
        },
        "middlewares": {
            "ts": [
                "atma-loader-ts:read"
            ]
        },
        "dynamicDependencies": [
            "postmask",
            "atma-io-middleware-base"
        ]
    }
}
